        "  z = jnp.argmax(z_logits, axis=1)\n",
        "  z = jnp.transpose(jax.nn.one_hot(z, num_classes=8192), (0, 3, 1, 2))\n",
        "  # Clearly we want to precompute - ugh. \n",
        "  B, S, H,W = z.shape\n",
        "  z_seq = z.reshape(B,H*W, S)\n",
        "  b['z_seq'] = z_seq\n",
